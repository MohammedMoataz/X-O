    <children>
        <AnchorPane fx:id="pane" prefHeight="600.0" prefWidth="600.0">
            <children>
                <Line endX="200.0" endY="550.0" startX="200.0" startY="50.0" />
                <Line endX="400.0" endY="550.0" startX="400.0" startY="50.0" />
                <Line endX="550.0" endY="200.0" startX="50.0" startY="200.0" />
                <Line endX="550.0" endY="400.0" startX="50.0" startY="400.0" />
